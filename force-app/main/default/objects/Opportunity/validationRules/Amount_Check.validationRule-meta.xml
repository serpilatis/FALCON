<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Amount_Check</fullName>
    <active>true</active>
    <description>Amount cannot be less than 20000</description>
    <errorConditionFormula>Amount &lt; 20000</errorConditionFormula>
    <errorDisplayField>Amount</errorDisplayField>
    <errorMessage>Minimum amaunt must be 20000</errorMessage>
</ValidationRule>
