<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Annual_Revenue_Check</fullName>
    <active>false</active>
    <errorConditionFormula>( AnnualRevenue  &gt; 100000000000) ||  AnnualRevenue &lt;0</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Annuel Revenue cannot exceed 100 billion</errorMessage>
</ValidationRule>
